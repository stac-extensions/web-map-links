      "wmts:encoding": "rest",
      "wmts:layer": "mosaic",
      "uriTemplate": "https://maps.example.com/wmts/rest/mosaic/{time}/{style}/{TileMatrixSet}/{TileMatrix}/{TileRow}/{TileCol}.png",
